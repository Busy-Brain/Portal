<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mk.portal.framework.entity.PageFileMappingEntity" table="page_file_mapping" catalog="portal">
        <id name="id" type="string">
            <column name="id" length="45" />
        </id>
        <property name="siteId" type="string">
            <column name="site_id" length="45" not-null="true" />
        </property>
        <property name="pageId" type="string">
            <column name="page_id" length="45" />
        </property>
        <property name="fileId" type="string">
            <column name="file_id" not-null="true" length="45"/>
        </property>
    </class>
</hibernate-mapping>
