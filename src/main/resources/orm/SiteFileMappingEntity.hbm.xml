<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.mk.portal.framework.entity.SiteFileMappingEntity" table="css_mapping" catalog="portal">
        <id name="id" type="string">
            <column name="id" length="45" />
        </id>
        <property name="siteId" type="string">
            <column name="site_id" length="45" not-null="true" />
        </property>
        <property name="fileId" type="string">
            <column name="page_id" length="60" />
        </property>
    </class>
</hibernate-mapping>
